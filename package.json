    "readable-stream": "~2.0.0",
    "request": "~2.69.0",
    "underscore": "~1.4.4",
    "validator": "~3.22.2",
    "xml2js": "0.2.7",
    "xmlbuilder": "0.4.3"
  },
  "devDependencies": {
    "browserify-fs": "^1.0.0",
    "coveralls": "^2.11.4",
    "grunt": "~0.4.2",
    "grunt-contrib-jshint": "~0.11.0",
    "grunt-devserver": "^0.6.0",
    "grunt-jsdoc": "~0.5.1",
    "grunt-mocha": "^0.4.12",
    "grunt-mocha-test": "^0.12.7",
    "grunt-nsp-package": "0.0.5",
    "istanbul": "^0.3.22",
